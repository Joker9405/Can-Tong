{
  "functions": {
    "api/route.js": {
      "runtime": "nodejs18.x",
      "includeFiles": "web/api/**"
    }
  },
  "rewrites": [
    { "source": "/api/ping", "destination": "/api/ping" },
    { "source": "/api/:fn", "destination": "/api/route?fn=:fn" },
    { "source": "/admin", "destination": "/web/admin.html" },
    { "source": "/", "destination": "/web/index.html" }
  ]
}
